<% if (errors.length > 0) { %>
<div class="row">
    <div class="col my-3">
        <div class="alert alert-danger">
            <% errors.forEach(erro => { %>
                <%= erro %>
            <% }); %>
        </div>
    </div>
</div>
<% } %>

<% if (sucess.length > 0) { %>
    <div class="row">
        <div class="col my-3">
            <div class="alert alert-success">
                <% sucess.forEach(sucess => { %>
                    <%= sucess %>
                <% }); %>
            </div>
        </div>
    </div>
    <% } %>